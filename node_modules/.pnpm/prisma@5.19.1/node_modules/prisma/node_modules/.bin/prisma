#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules/prisma/build/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules/prisma/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules/prisma/build/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules/prisma/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/prisma@5.19.1/node_modules:/home/hp/.cache/pnpm/dlx/b4kwuke4fcm2ppufregi7l7yfi/191ff67f063-35c3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/index.js" "$@"
else
  exec node  "$basedir/../../build/index.js" "$@"
fi
